@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix :     <http://tardis.ed.ac.uk/~wwaites/units#> .

:UnitOfMeasure a owl:Class .

:mcg a :UnitOfMeasure ;
    rdfs:label "µg" ;
    rdfs:comment "microgram" .

:mg a :UnitOfMeasure ;
    rdfs:label "mg" ;
    rdfs:comment "milligram" .

:g a :UnitOfMeasure ;
    rdfs:label "g" ;
    rdfs:comment "gram" .

:kg a :UnitOfMeasure ;
    rdfs:label "kg" ;
    rdfs:comment "kilogram" .

:t a :UnitOfMeasure ;
    rdfs:label "t" ;
    rdfs:comment "metric tonne" .

:lb a :UnitOfMeasure ;
    rdfs:label "lb" ;
    rdfs:comment "pound" .

:stone a :UnitOfMeasure ;
    rdfs:label "st" ;
    rdfs:comment "stone" .


