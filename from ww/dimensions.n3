@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix u:     <http://tardis.ed.ac.uk/~wwaites/units#> .
@prefix dim:     <http://tardis.ed.ac.uk/~wwaites/dimensions#> .

dim:Dimension a rdfs:Class ;
    rdfs:subClassOf owl:DatatypeProperty .

dim:weightInMicrograms a dim:Dimension ;
    rdfs:range u:mcg .

dim:weightInMilligrams a dim:Dimension ;
    rdfs:range u:mg .

dim:weightInGrams a dim:Dimension ;
    rdfs:range u:g .

dim:weightInKilograms a dim:Dimension ;
    rdfs:range u:kg .

dim:weightInTonnes a dim:Dimension ;
    rdfs:range u:t .

dim:weightInPounds a dim:Dimension ;
    rdfs:range u:lb .

dim:weightInStone a dim:Dimension ;
    rdfs:range u:stone .
